# Multi-Agent SIEM Framework - Environment Variables
# Copy this file to .env and fill in your actual values

# ============================================================================
# Gemini API Configuration
# ============================================================================
GEMINI_API_KEY=your_gemini_api_key_here

# ============================================================================
# Splunk Configuration
# ============================================================================
SPLUNK_HOST=192.168.154.129
SPLUNK_PORT=8089
SPLUNK_USER=admin
SPLUNK_PASSWORD=your_splunk_password
SPLUNK_VERIFY_SSL=false

# ============================================================================
# SSH Configuration (for remote attack execution)
# ============================================================================
SSH_HOST=192.168.154.129
SSH_PORT=22
SSH_USER=root
SSH_PASSWORD=your_ssh_password
# SSH_KEY_PATH=/path/to/ssh/private/key  # Alternative to password

# ============================================================================
# MISP Configuration (Optional)
# ============================================================================
MISP_URL=https://your-misp-instance.com
MISP_API_KEY=your_misp_api_key

# ============================================================================
# TAXII Configuration (Optional)
# ============================================================================
TAXII_URL=https://your-taxii-server.com
TAXII_USERNAME=your_username
TAXII_PASSWORD=your_password

# ============================================================================
# OpenCTI Configuration (Optional)
# ============================================================================
OPENCTI_URL=https://your-opencti-instance.com
OPENCTI_API_KEY=your_opencti_api_key
