# Multi-Agent SIEM Framework - Environment Variables Template
# Copy this to .env and fill in your actual values

# Application Settings
APP_NAME="Multi-Agent SIEM Framework"
APP_VERSION="1.0.0"
DEBUG=false
ENVIRONMENT=development

# Database Configuration
DATABASE_URL=postgresql+asyncpg://siem_user:siem_password@localhost:5432/multi_agent_siem

# Redis Configuration
REDIS_URL=redis://localhost:6379
REDIS_DB=0
REDIS_PASSWORD=

# CTI Sources API Keys (CRITICAL - Fill these in!)
MISP_API_KEY=your_misp_api_key_here
MISP_URL=https://your-misp-instance.com
MISP_VERIFY_CERT=true

TAXII_URL=https://your-taxii-server.com
TAXII_USERNAME=your_taxii_username
TAXII_PASSWORD=your_taxii_password

OPENCTI_URL=https://your-opencti-instance.com
OPENCTI_API_KEY=your_opencti_api_key

# SIEM Integration
SPLUNK_HOST=https://your-splunk.com
SPLUNK_API_KEY=your_splunk_api_key
ELASTICSEARCH_HOST=https://your-elasticsearch.com
ELASTICSEARCH_API_KEY=your_elasticsearch_key

# LLM Integration
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_claude_api_key_here

# Agent Configuration
COLLECTOR_INTERVAL=300
EXTRACTION_TIMEOUT=120
RULE_GENERATION_BATCH_SIZE=10
MAX_CONCURRENT_OPERATIONS=5

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=json
